#: FamilyTree/FamilyTree.gpr.py:4 FamilyTree/FamilyTree.gpr.py:5
msgid "Produces a graphical family tree."
msgstr "Gera uma árvore genealógica gráfica"

#: FamilyTree/FamilyTree.py:90 FamilyTree/FamilyTree.py:102
#, python-format
msgid "Family Tree for %s"
msgstr "Arvore genealógica para %s"

#: FamilyTree/FamilyTree.py:640 FamilyTree/FamilyTree.py:684
msgid "Anonymous"
msgstr "Anônimo"

#: FamilyTree/FamilyTree.py:837 FamilyTree/FamilyTree.py:911
msgid "born"
msgstr "nascido(a)"

#: FamilyTree/FamilyTree.py:839 FamilyTree/FamilyTree.py:913
msgid "baptised"
msgstr "batisado(a)"

#: FamilyTree/FamilyTree.py:841 FamilyTree/FamilyTree.py:915
msgid "died"
msgstr "falecido(a)"

#: FamilyTree/FamilyTree.py:843 FamilyTree/FamilyTree.py:917
msgid "buried"
msgstr "sepultado(a)"

#: FamilyTree/FamilyTree.py:845 FamilyTree/FamilyTree.py:919
msgid "cremated"
msgstr "cremado(a)"

#: FamilyTree/FamilyTree.py:847 FamilyTree/FamilyTree.py:921
msgid "married"
msgstr "casado(a)"

#: FamilyTree/FamilyTree.py:849 FamilyTree/FamilyTree.py:923
msgid "divorced"
msgstr "divorciado(a)"

#: FamilyTree/FamilyTree.py:851 FamilyTree/FamilyTree.py:925
msgid "resident"
msgstr "residência"

#: FamilyTree/FamilyTree.py:858 FamilySheet/FamilySheet.py:769
#, python-format
msgid "on %(ymd_date)s"
msgstr "em %(ymd_date)s"

#: FamilyTree/FamilyTree.py:860 FamilySheet/FamilySheet.py:771
#, python-format
msgid "in %(ym_date)s"
msgstr "em %(ym_date)s"

#: FamilyTree/FamilyTree.py:862 FamilySheet/FamilySheet.py:773
#, python-format
msgid "in %(y_date)s"
msgstr "em %(y_date)s"

#: FamilyTree/FamilyTree.py:866 FamilySheet/FamilySheet.py:777
#, python-format
msgid "on %(placeholder)s"
msgstr "em %(placeholder)s"

#: FamilyTree/FamilyTree.py:869 FamilySheet/FamilySheet.py:779
#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "em %(placeholder)s (%(partial)s)"

#: FamilyTree/FamilyTree.py:886 FamilySheet/FamilySheet.py:824
#: FamilySheet/FamilySheet.py:826
#, python-format
msgid "in %(place)s"
msgstr "em %(place)s"

#: FamilyTree/FamilyTree.py:897 FamilyTree/FamilyTree.py:971
#, python-format
msgid "(%(description)s)"
msgstr "(%(description)s)"

#: FamilyTree/FamilyTree.py:978 FamilyTree/FamilyTree.py:1044
msgid "The center family for the tree"
msgstr "A família principal para o relatório"

#: FamilyTree/FamilyTree.py:981 FamilyTree/FamilyTree.py:1047
msgid "Ancestor Generations"
msgstr "Número máximo de gerações de ascendentes"

#: FamilyTree/FamilyTree.py:982 FamilyTree/FamilyTree.py:1048
msgid "The number of ancestor generations to include in the tree"
msgstr "Número de gerações a serem incluídas na árvore"

#: FamilyTree/FamilyTree.py:985 FamilyTree/FamilyTree.py:1051
msgid "Descendant Generations"
msgstr "Número máximo de gerações de descendentes"

#: FamilyTree/FamilyTree.py:986 FamilyTree/FamilyTree.py:1052
msgid "The number of descendant generations to include in the tree"
msgstr "Número de gerações a serem incluídas na árvore"

#: FamilyTree/FamilyTree.py:990 FamilyTree/FamilyTree.py:1060
msgid "Whether to scale to fit on a single page."
msgstr "Ajustar para caber em uma única página"

#: FamilyTree/FamilyTree.py:995 FamilyTree/FamilyTree.py:1065
msgid "No color"
msgstr "Sem cor"

#: FamilyTree/FamilyTree.py:997 FamilyTree/FamilyTree.py:1067
msgid "First generation"
msgstr "Primeira geração"

#: FamilyTree/FamilyTree.py:998 FamilyTree/FamilyTree.py:1068
msgid "Second generation"
msgstr "Segunda geração"

#: FamilyTree/FamilyTree.py:999 FamilyTree/FamilyTree.py:1069
msgid "Third generation"
msgstr "Terceira geração"

#: FamilyTree/FamilyTree.py:1000 FamilyTree/FamilyTree.py:1070
msgid "Male line"
msgstr "Linhagem masculina"

#: FamilyTree/FamilyTree.py:1001 FamilyTree/FamilyTree.py:1071
msgid "Male line and illegitimate children"
msgstr "Linhagem masculina e filhos ilegitimos"

#: FamilyTree/FamilyTree.py:1002 FamilyTree/FamilyTree.py:1072
msgid "Female line"
msgstr "Linhagem feminina"

#: FamilyTree/FamilyTree.py:1005 FamilyTree/FamilyTree.py:1075
msgid "Shuffle colors"
msgstr "Embaralhar cores"

#: FamilyTree/FamilyTree.py:1006 FamilyTree/FamilyTree.py:1076
msgid "Whether to shuffle colors or order them in rainbow fashion."
msgstr "Embaralhar as cores ou ordenar como arco-iris"

#: FamilyTree/FamilyTree.py:1014 FamilySheet/FamilySheet.py:880
msgid "Replace first name with call name"
msgstr "Substituir o primeiro nome pelo nome vocativo"

#: FamilyTree/FamilyTree.py:1015 FamilySheet/FamilySheet.py:881
msgid "Underline call name in first name / add call name to first name"
msgstr ""
"Sublinhar o nome vocativo no primeiro nome / adicionar nome vocativo ao "
"primeiro nome"

#: FamilyTree/FamilyTree.py:1018 FamilyTree/FamilyTree.py:1091
msgid "Include Occupation"
msgstr "Incluir profissão"

#: FamilyTree/FamilyTree.py:1021 FamilyTree/FamilyTree.py:1097
msgid "Include Residence"
msgstr "Incluir residência"

#: FamilyTree/FamilyTree.py:1024 FamilyTree/FamilyTree.py:1100
msgid "Print event data (dead person)"
msgstr "Imprimir dados de evento (pessoa falecida)"

#: FamilyTree/FamilyTree.py:1027 FamilyTree/FamilyTree.py:1035
#: FamilyTree/FamilyTree.py:1103 FamilyTree/FamilyTree.py:1111
msgid "Year only"
msgstr "Somente ano"

#: FamilyTree/FamilyTree.py:1028 FamilyTree/FamilyTree.py:1036
#: FamilyTree/FamilyTree.py:1104 FamilyTree/FamilyTree.py:1112
msgid "Full date"
msgstr "Data completa"

#: FamilyTree/FamilyTree.py:1029 FamilyTree/FamilyTree.py:1037
#: FamilyTree/FamilyTree.py:1105 FamilyTree/FamilyTree.py:1113
msgid "Full date and place"
msgstr "Data e local completo"

#: FamilyTree/FamilyTree.py:1032 FamilyTree/FamilyTree.py:1108
msgid "Print event data (living person)"
msgstr "Imprimir dados de evento (pessoa viva)"

#: FamilyTree/FamilyTree.py:1040 FamilyTree/FamilyTree.py:1116
msgid "Fall back to baptism if birth event missing"
msgstr "Exibir evento de batismo se nascimento faltando"

#: FamilyTree/FamilyTree.py:1043 FamilyTree/FamilyTree.py:1119
msgid "Fall back to burial or cremation if death event missing"
msgstr "Exibir evento de sepultamento ou cremação se falecimento faltando"

#: FamilyTree/FamilyTree.py:1052 FamilyTree/FamilyTree.py:1131
msgid "Include event description"
msgstr "Incluir descrição de eventos"

#: FamilyTree/FamilyTree.py:1055 FamilyTree/FamilyTree.py:1134
msgid "Text Options"
msgstr "Opções de texto"

#: FamilyTree/FamilyTree.py:1083 FamilyTree/FamilyTree.py:1162
msgid "The style used for names."
msgstr "O estilo usado para nomes."

#: FamilyTree/FamilyTree.py:1091 FamilyTree/FamilyTree.py:1170
msgid "The style used for data (birth, death, marriage, divorce)."
msgstr ""
"O estilo usado para dados (nascimento, falecimento, casamento, divórcio."
