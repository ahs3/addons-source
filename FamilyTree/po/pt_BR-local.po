#, python-format
msgid "Family Tree for %s"
msgstr "Arvore genealógica para %s"

msgid "Anonymous"
msgstr "Anônimo"

msgid "born"
msgstr "nascido(a)"

msgid "baptised"
msgstr "batisado(a)"

msgid "died"
msgstr "falecido(a)"

msgid "buried"
msgstr "sepultado(a)"

msgid "cremated"
msgstr "cremado(a)"

msgid "married"
msgstr "casado(a)"

msgid "divorced"
msgstr "divorciado(a)"

msgid "resident"
msgstr "residência"

#, python-format
msgid "on %(ymd_date)s"
msgstr "em %(ymd_date)s"

#, python-format
msgid "in %(ym_date)s"
msgstr "em %(ym_date)s"

#, python-format
msgid "in %(y_date)s"
msgstr "em %(y_date)s"

#, python-format
msgid "on %(placeholder)s"
msgstr "em %(placeholder)s"

#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "em %(placeholder)s (%(partial)s)"

#, python-format
msgid "in %(place)s"
msgstr "em %(place)s"

#, python-format
msgid "(%(description)s)"
msgstr "(%(description)s)"

msgid "The center family for the tree"
msgstr "A família principal para o relatório"

msgid "Ancestor Generations"
msgstr "Número máximo de gerações de ascendentes"

msgid "The number of ancestor generations to include in the tree"
msgstr "Número de gerações a serem incluídas na árvore"

msgid "Descendant Generations"
msgstr "Número máximo de gerações de descendentes"

msgid "The number of descendant generations to include in the tree"
msgstr "Número de gerações a serem incluídas na árvore"

msgid "Whether to scale to fit on a single page."
msgstr "Ajustar para caber em uma única página"

msgid "No color"
msgstr "Sem cor"

msgid "First generation"
msgstr "Primeira geração"

msgid "Second generation"
msgstr "Segunda geração"

msgid "Third generation"
msgstr "Terceira geração"

msgid "Male line"
msgstr "Linhagem masculina"

msgid "Male line and illegitimate children"
msgstr "Linhagem masculina e filhos ilegitimos"

msgid "Female line"
msgstr "Linhagem feminina"

msgid "Shuffle colors"
msgstr "Embaralhar cores"

msgid "Whether to shuffle colors or order them in rainbow fashion."
msgstr "Embaralhar as cores ou ordenar como arco-iris"

msgid "Replace first name with call name"
msgstr "Substituir o primeiro nome pelo nome vocativo"

msgid "Underline call name in first name / add call name to first name"
msgstr ""
"Sublinhar o nome vocativo no primeiro nome / adicionar nome vocativo ao "
"primeiro nome"

msgid "Include Occupation"
msgstr "Incluir profissão"

msgid "Include Residence"
msgstr "Incluir residência"

msgid "Print event data (dead person)"
msgstr "Imprimir dados de evento (pessoa falecida)"

msgid "Year only"
msgstr "Somente ano"

msgid "Full date"
msgstr "Data completa"

msgid "Full date and place"
msgstr "Data e local completo"

msgid "Print event data (living person)"
msgstr "Imprimir dados de evento (pessoa viva)"

msgid "Fall back to baptism if birth event missing"
msgstr "Exibir evento de batismo se nascimento faltando"

msgid "Fall back to burial or cremation if death event missing"
msgstr "Exibir evento de sepultamento ou cremação se falecimento faltando"

msgid "Include event description"
msgstr "Incluir descrição de eventos"

msgid "Text Options"
msgstr "Opções de texto"

msgid "The style used for names."
msgstr "O estilo usado para nomes."

msgid "The style used for data (birth, death, marriage, divorce)."
msgstr ""
"O estilo usado para dados (nascimento, falecimento, casamento, divórcio."

msgid "Produces a graphical family tree."
msgstr "Gera uma árvore genealógica gráfica"
