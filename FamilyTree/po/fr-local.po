#, python-format
msgid "Family Tree for %s"
msgstr "Arbre familial de %s"

msgid "Paper too small"
msgstr "Papier trop petit"

msgid "Some elements may not appear or be badly rendered."
msgstr "Certains éléments peuvent ne pas apparaître ou être mal rendus."

msgid "Anonymous"
msgstr "Anonyme"

msgid "born"
msgstr "né(e)"

msgid "baptised"
msgstr "baptisé(e)"

msgid "died"
msgstr "décédé(e)"

msgid "buried"
msgstr "inhumé(e)"

msgid "cremated"
msgstr "incinéré(e)"

msgid "married"
msgstr "marié(s)"

msgid "divorced"
msgstr "divorcé(e)"

msgid "resident"
msgstr "habitant(e)"

#, python-format
msgid "on %(ymd_date)s"
msgstr "le %(ymd_date)s"

#, python-format
msgid "in %(ym_date)s"
msgstr "en %(ym_date)s"

