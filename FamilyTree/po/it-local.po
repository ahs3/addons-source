#, python-format
msgid "Family Tree for %s"
msgstr "Albero genealogico per %s"

msgid "Anonymous"
msgstr "Anonimo"

msgid "born"
msgstr "nato"

msgid "baptised"
msgstr "battezzato"

msgid "died"
msgstr "deceduto"

msgid "buried"
msgstr "seppellito"

msgid "cremated"
msgstr "cremato"

msgid "married"
msgstr "sposato"

msgid "divorced"
msgstr "divorziato"

msgid "resident"
msgstr "residente"

#, python-format
msgid "on %(ymd_date)s"
msgstr "il %(ymd_date)s"

#, python-format
msgid "in %(ym_date)s"
msgstr "il %(ym_date)s"

#, python-format
msgid "in %(y_date)s"
msgstr "nel %(y_date)s"

#, python-format
msgid "on %(placeholder)s"
msgstr "in %(placeholder)s"

#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "in %(placeholder)s (%(partial)s)"

#, python-format
msgid "in %(place)s"
msgstr "in %(place)s"

#, python-format
msgid "(%(description)s)"
msgstr "(%(description)s)"

msgid "The center family for the tree"
msgstr "La famiglia principale dell'albero"

msgid "Ancestor Generations"
msgstr "Generazioni ascendenti"

msgid "The number of ancestor generations to include in the tree"
msgstr "Il numero di generazioni di antenati da includere nell'albero"

msgid "Descendant Generations"
msgstr "Generazioni discendenti"

msgid "The number of descendant generations to include in the tree"
msgstr "Il numero di generazioni di discendenti da includere nell'albero"

msgid "Kekule number of husband"
msgstr "Numero Kekule di mariti"

msgid ""
"The Kekule number of the husband (central family). Set 0 to not show Kekule "
"numbers"
msgstr ""
"Il numero Kekule del marito (famiglia principale). Scegli 0 per non mostrarlo"

msgid "Scale to fit on a single page"
msgstr "Ridimensiona su una singola pagina"

msgid "Whether to scale to fit on a single page."
msgstr "Se ridimensionare per farlo entrare in una sola pagina."

msgid "No color"
msgstr "Nessun colore"

msgid "First generation"
msgstr "Prima generazione"

msgid "Second generation"
msgstr "Seconda generazione"

msgid "Third generation"
msgstr "Terza generazione"

msgid "Male line"
msgstr "Linea maschile"

msgid "Male line and illegitimate children"
msgstr "Linea maschile e figli illegittimi"

msgid "Female line"
msgstr "Linea femminile"

msgid "Shuffle colors"
msgstr "Mischia i colori"

msgid "Whether to shuffle colors or order them in rainbow fashion."
msgstr "Se mischiare i colori o ordinarli a mo' di arcobaleno."

msgid "Replace first name with call name"
msgstr "Sostituisci il nome con lo pseudonimo"

msgid "Underline call name in first name / add call name to first name"
msgstr "Sottolinea lo pseudonimo nel nome / aggiungi lo pseudonimo al nome"

msgid "Include Occupation"
msgstr "Includi occupazione"

msgid "Include Residence"
msgstr "Includi residenza"

msgid "Print event data (dead person)"
msgstr "Stampa dati sugli eventi (dei morti)"

msgid "Year only"
msgstr "Solo anno"

msgid "Full date"
msgstr "Data completa"

msgid "Full date and place"
msgstr "Data completa e luogo"

msgid "Print event data (living person)"
msgstr "Stampa i dati sugli eventi (dei vivi)"

msgid "Fall back to baptism if birth event missing"
msgstr "Usa il battesimo se manca l'evento nascita"

msgid "Fall back to burial or cremation if death event missing"
msgstr "Usa sepoltura o cremazione se manca l'evento morte"

msgid "Protect private items"
msgstr "Proteggi gli elementi privati"

msgid "Include event description"
msgstr "Includi descrizione dell'evento"

msgid "Text Options"
msgstr "Opzioni di testo"

msgid "The style used for names."
msgstr "Lo stile usato per i nomi."

msgid "The style used for data (birth, death, marriage, divorce)."
msgstr "Lo stile usato per i dati (nascita, morte, matrimonio, divorzio)."

msgid "Produces a graphical family tree."
msgstr "Genera un albero genealogico grafico."
