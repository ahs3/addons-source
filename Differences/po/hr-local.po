#: Differences/differences.py:95 Differences/differences.gpr.py:4
msgid "Database Differences Report"
msgstr "Izvještaj o razlikama između baza podataka"

#: Differences/differences.py:134
msgid "Differences between Database and File"
msgstr "Razlike između baze podataka i datoteke"

#: Differences/differences.py:161
msgid "No differences"
msgstr "Nema razlika"

#: Differences/differences.py:168
msgid "Missing items in File that are added in Database"
msgstr "U datoteci nedostaju elementi koji su dodani u bazu podataka"

#: Differences/differences.py:174
#, python-format
msgid "Missing %s: %s"
msgstr "Nedostaje %s: %s"

#: Differences/differences.py:178
msgid "Nothing missing"
msgstr "Ništa ne nedostaje"

#: Differences/differences.py:185
msgid "Added items in File that are missing in Database"
msgstr "U datoteci dodani elementi koji nedostaju u bazi podataka"

#: Differences/differences.py:191
#, python-format
msgid "Added %s: %s "
msgstr "Dodano %s: %s "

#: Differences/differences.py:195
msgid "Nothing added"
msgstr "Ništa nije dodano"

#: Differences/differences.py:308
msgid "Family Tree file"
msgstr "Datoteka rodoslova"

#: Differences/differences.py:310
msgid "Select a .gpkg or .gramps file"
msgstr "Odaberi .gpkg ili .gramps datoteku"

#: Differences/differences.py:313
msgid "Show items that are different"
msgstr "Prikaži elemente koji se razlikuju"

#: Differences/differences.py:314
msgid "Include items that are different"
msgstr "Uključi elemente koji se razlikuju"

#: Differences/differences.py:317
msgid "Show items missing from file"
msgstr "Prikaži elemente koji nedostaju u datoteci"

#: Differences/differences.py:318
msgid "Include items not in file but in database"
msgstr ""
"Uključi elemente koji nedostaju u datoteci, ali se nalaze u bazi podataka"

#: Differences/differences.py:321
msgid "Show items added in file"
msgstr "Prikaži elemente koji su dodani u datoteku"

#: Differences/differences.py:322
msgid "Include items in file but not in database"
msgstr ""
"Uključi elemente koji nedostaju u bazi podataka, ali se nalaze u datoteci"

#: Differences/differences.gpr.py:5
msgid "Compares an external database with the current one."
msgstr "Uspoređuje vanjsku bazu podataka s trenutačnom."
