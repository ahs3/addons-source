#: FamilySheet/FamilySheet.gpr.py:3 FamilySheet/FamilySheet.py:126
#: FamilySheet/FamilySheet.gpr.py:4
msgid "Family Sheet"
msgstr "Семейные страницы"

#: FamilySheet/FamilySheet.gpr.py:4 FamilySheet/FamilySheet.gpr.py:6
msgid ""
"Produces a family sheet showing full information about a person and his/her "
"partners and children."
msgstr ""
"Создаёт семейную страницу со всей информацией о лице, его родителях и детях."

#: FamilySheet/FamilySheet.py:232 FamilySheet/FamilySheet.py:275
#, python-format
msgid "\\u2192 %s"
msgstr "\\u2192 %s"

#: FamilySheet/FamilySheet.py:396 FamilySheet/FamilySheet.py:397
#, python-format
msgid "Address (%(date)s): %(location)s"
msgstr "Адрес (%(date)s): %(location)s"

#: FamilySheet/FamilySheet.py:400 FamilySheet/FamilySheet.py:401
#, python-format
msgid "Address: %(location)s"
msgstr "Адрес: %(location)s"

#: FamilySheet/FamilySheet.py:451 FamilySheet/FamilySheet.py:469
#, python-format
msgid "; %(type)s: %(value)s"
msgstr "; %(type)s: %(value)s"

#: FamilySheet/FamilySheet.py:539 FamilySheet/FamilySheet.py:557
msgid "Source references:"
msgstr "Ссылки на источник:"

#: FamilySheet/FamilySheet.py:557 FamilySheet/FamilySheet.py:575
#, python-format
msgid ", page %s"
msgstr ", страница %s"

#: FamilySheet/FamilySheet.py:570 FamilySheet/FamilySheet.py:588
msgid "Notes:"
msgstr "Заметки:"

#: FamilySheet/FamilySheet.py:749 FamilySheet/FamilySheet.py:769
#, python-format
msgid "on %(ymd_date)s"
msgstr "в %(ymd_date)s"

#: FamilySheet/FamilySheet.py:751 FamilySheet/FamilySheet.py:771
#, python-format
msgid "in %(ym_date)s"
msgstr "в %(ym_date)s"

#: FamilySheet/FamilySheet.py:753 FamilySheet/FamilySheet.py:773
#, python-format
msgid "in %(y_date)s"
msgstr "в %(y_date)s"

#: FamilySheet/FamilySheet.py:757 FamilySheet/FamilySheet.py:777
#, python-format
msgid "on %(placeholder)s"
msgstr "в %(placeholder)s"

#: FamilySheet/FamilySheet.py:759 FamilySheet/FamilySheet.py:779
#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "в %(placeholder)s (%(partial)s)"

#: FamilySheet/FamilySheet.py:804 FamilySheet/FamilySheet.py:806
#: FamilySheet/FamilySheet.py:824 FamilySheet/FamilySheet.py:826
#, python-format
msgid "in %(place)s"
msgstr "в %(place)s"

#: FamilySheet/FamilySheet.py:844 FamilySheet/FamilySheet.py:864
msgid "Center person"
msgstr "Главное лицо"

#: FamilySheet/FamilySheet.py:846 FamilySheet/FamilySheet.py:866
msgid "The person whose partners and children are printed"
msgstr "Лицо, чьи родители и дети будут отображены"

#: FamilySheet/FamilySheet.py:849 FamilySheet/FamilySheet.py:869
msgid "Print sheets for"
msgstr "Создать страницы для"

#: FamilySheet/FamilySheet.py:851 FamilySheet/FamilySheet.py:871
msgid "Center person only"
msgstr "Только главное лицо"

#: FamilySheet/FamilySheet.py:852 FamilySheet/FamilySheet.py:872
msgid "Center person and descendants in side branches"
msgstr "Главное лицо и потомки в соседних ветках"

#: FamilySheet/FamilySheet.py:853 FamilySheet/FamilySheet.py:873
msgid "Center person and all descendants"
msgstr "Главное лицо и все потомки"

#: FamilySheet/FamilySheet.py:854 FamilySheet/FamilySheet.py:874
msgid "Whether to include descendants, and which ones."
msgstr "Включать ли потомков и каких."

#: FamilySheet/FamilySheet.py:860 FamilySheet/FamilySheet.py:880
msgid "Replace first name with call name"
msgstr "Заменить имя на прозвище"

#: FamilySheet/FamilySheet.py:861 FamilySheet/FamilySheet.py:881
msgid "Underline call name in first name / add call name to first name"
msgstr "Подчеркнуть прозвище в имени / добавить прозвище в имя"

#: FamilySheet/FamilySheet.py:862 FamilySheet/FamilySheet.py:882
msgid "Whether to include call name, and how."
msgstr "Включать ли прозвище и как."

#: FamilySheet/FamilySheet.py:865 FamilySheet/FamilySheet.py:885
msgid "Print placeholders for missing information"
msgstr "Оставлять места для пропущенной информации"

#: FamilySheet/FamilySheet.py:889 FamilySheet/FamilySheet.py:909
msgid "The basic style used for the text display"
msgstr "Основной стиль для отображения текста"

#: FamilySheet/FamilySheet.py:899 FamilySheet/FamilySheet.py:919
msgid "The style used for the page key on the top"
msgstr "Стиль для номера страницы сверху"

#: FamilySheet/FamilySheet.py:908 FamilySheet/FamilySheet.py:928
msgid "The style used for names"
msgstr "Стиль для имён"

#: FamilySheet/FamilySheet.py:918 FamilySheet/FamilySheet.py:938
msgid "The style used for numbers"
msgstr "Стиль для чисел"

#: FamilySheet/FamilySheet.py:928 FamilySheet/FamilySheet.py:948
msgid "The style used for footnotes (notes and source references)"
msgstr "Стиль для примечаний (заметок и ссылок на источник)"
