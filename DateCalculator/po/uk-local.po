msgid "Reference Date or Date Range"
msgstr "Опорна дата або діапазон дат"

msgid "a valid Gramps date"
msgstr "дійсна дата Gramps"

msgid "Date or offset ±y or ±y, m, d"
msgstr "Дата або зсув ±р або ±р, м, д"

msgid ""
"1. a Date\n"
"2. a positive or negative number, representing years\n"
"3. a positive or negative list of values, representing years, months, days"
msgstr ""
"1. Дата\n"
"2. Додатне або від'ємне число, що позначає роки\n"
"3. Додатний або від'ємний список значень, що позначає роки, місяці, дні"

msgid "Result"
msgstr "Результат"

msgid "Calculate"
msgstr "Обчислити"

msgid "Copy"
msgstr "Копіювати"

msgid "Error: invalid date for first expression"
msgstr "Помилка: недійсна дата для першого виразу"

msgid "Error: invalid offset for second expression"
msgstr "Помилка: недійсний зсув для другого виразу"

msgid "Error: at least one expression must be a date"
msgstr "Помилка: принаймні один вираз має бути датою"

msgid "Enter an expression in the entries above and click Calculate."
msgstr "Введіть вираз у поле вище та натисніть 'Обчислити'."

msgid "Date Calculator"
msgstr "Калькулятор дат"

msgid "Perform date math calculations"
msgstr "Виконує математичні обчислення з датами"
