msgid "Reference Date or Date Range"
msgstr "Data de referência ou intervalo de datas"

msgid "a valid Gramps date"
msgstr "uma data Gramps válida"

msgid "Date or offset ±y or ±y, m, d"
msgstr "Data ou desvio ±y ou ±y, m, d"

msgid ""
"1. a Date\n"
"2. a positive or negative number, representing years\n"
"3. a positive or negative list of values, representing years, months, days"
msgstr ""
"1. uma data\n"
"2. um número positivo ou negativo, representando anos\n"
"3. uma lista de valores positivos ou negativos, representando anos, meses, "
"dias"

msgid "Result"
msgstr "Resultado"

msgid "Calculate"
msgstr "Calcular"

msgid "Copy"
msgstr "Copiar"

msgid "Error: invalid date for first expression"
msgstr "Erro: data inválida para a primeira expressão"

msgid "Error: invalid offset for second expression"
msgstr "Erro: desvio inválido para a segunda expressão"

msgid "Error: at least one expression must be a date"
msgstr "Erro: pelo menos uma das expressões tem de ser uma data"

msgid "Enter an expression in the entries above and click Calculate."
msgstr "Insira uma expressão nos campos acima e clique em Calcular."

msgid "Date Calculator"
msgstr "Calculadora de datas"

msgid "Perform date math calculations"
msgstr "Efectuar cálculos matemáticos de datas"
