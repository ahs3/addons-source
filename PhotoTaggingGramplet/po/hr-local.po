msgid "https://www.gramps-project.org/wiki/index.php/Photo_Tagging_Gramplet"
msgstr "https://www.gramps-project.org/wiki/index.php/Photo_Tagging_Gramplet"

msgid "Replace existing references to the person being assigned without asking"
msgstr "Zamijeni postojeće reference s određenom osobom, bez pitanja"

msgid "Face detection"
msgstr "Pronalaženje lica"

msgid "Minimum face width (px)"
msgstr "Minimala širina lica (px)"

msgid "Minimum face height (px)"
msgstr "Minimala visina lica (px)"

msgid "Detect faces inside existing boxes"
msgstr "Pronađi lica unutar postojećih okvira"

msgid "Sensitivity (1 min .. 20 max)"
msgstr "Osjetljivost (1 min. … 20 maks.)"

msgid "Add Person"
msgstr "Dodaj osobu"

msgid "Clear Reference"
msgstr "Ukloni referencu"

msgid "Remove Selection"
msgstr "Ukloni odabir"

msgid "Edit referenced Person"
msgstr "Uredi referneciranu osobu"

msgid "Zoom In"
msgstr "Uvećaj"

msgid "Zoom Out"
msgstr "Umanji"

msgid "Detect faces"
msgstr "Pronađi lica"

msgid "Detect faces (OpenCV module required)"
msgstr "Pronađi lica (potreban je OpenCV modul)"

msgid "Set as active person"
msgstr "Postavi kao aktivnu osobu"

msgid "_Select"
msgstr "_Odaberi"

#, python-brace-format
msgid "Replace to {0}"
msgstr "Zamijeni na {0}"

msgid "Detecting faces..."
msgstr "Pronalaženje lica …"

msgid "Detection finished"
msgstr "Pronalaženje je završeno"

#, python-brace-format
msgid "Another region of this image is associated with {name}. Remove it?"
msgstr ""
"Jedno drugo područje ove slike povezano je s osobom {name}. Želiš li ga "
"ukloniti?"

#, python-brace-format
msgid ""
"{count} other regions of this image are associated with {name}. Remove them?"
msgstr ""
"{count} druga područja ove slike povezana su s osobom {name}. Želiš li ih "
"ukloniti?"

#, python-format
msgid "Male person|Dead at %s"
msgstr "Umro %s"

#, python-format
msgid "Female person|Dead at %s"
msgstr "Umrla %s"

#, python-format
msgid "Unknown gender person|Dead at %s"
msgstr "Umro/umrla %s"

msgid "Photo Tagging"
msgstr "Označivanje slika"

msgid "Gramplet for tagging people in photos"
msgstr "Gramplet za označivanje osoba u slikama"
