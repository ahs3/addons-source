msgid "Descendant Book"
msgstr "Kniha potomkov"

msgid "Produces one or more descendant reports based on a supplied query."
msgstr ""
"Vytvára jednu alebo viac správ o potomkoch na základe dodanej požiadavky."

msgid "Detailed Descendant Book"
msgstr "Podrobná kniha potomkov"

msgid ""
"Produces one or more detailed descendant reports based on a supplied query."
msgstr ""
"Vytvára jednu alebo viac podrobných správ o potomkoch na základe dodanej "
"požiadavky."

#, python-format
msgid "sp. see  %(reference)s : %(spouse)s"
msgstr "manž. pozri   %(reference)s : %(spouse)s\""

#, python-format
msgid "see report: %(report)s, ref: %(reference)s : %(person)s & %(spouse)s"
msgstr "pozri správu: %(report)s, ref: %(reference)s : %(person)s & %(spouse)s"

msgid "Descendants Report"
msgstr "Správa o potomkoch"

#, python-format
msgid "Writing %s reports..."
msgstr "Písanie %s správ..."

#, python-brace-format
msgid "{report_count}. Descendants of {name}"
msgstr "(report_count). Potomkovia osoby (name)"

msgid "Select filter to restrict people that appear in the report"
msgstr "Vyberte filter na vymedzenie ľudí, ktorí sa objavia v správe"

msgid "Whether to show duplicate family trees in the report."
msgstr "Či sa majú v správe zobraziť duplicitné rodinné stromy."

msgid "The style used for the title of the page."
msgstr "Štýl použitý pre titul strany."

msgid "The style used for the table of contents header."
msgstr "Štýl použitý pre záhlavie obsahu."

msgid "The style used for the table of contents detail."
msgstr "Štýl použitý pre detail obsahu."

msgid "Detailed Descendants Report"
msgstr "Podrobná správa o potomkoch"

#, python-format
msgid "Generating %s report references..."
msgstr "Generovanie %s odkazov správy..."

#, python-format
msgid "%(report_count)s. Descendant Report for %(person_name)s"
msgstr "%(report_count)s. Správa o potomkoch pre %(person_name)s"

msgid "Index of Places"
msgstr "Index miest"

msgid "Index of Dates"
msgstr "Index dátumov"

msgid "Index of Names"
msgstr "Index mien"

msgid "Report, Generation, Person, Name"
msgstr "Správa, Generácia, Osoba, Meno"

#, python-brace-format
msgid "See Report : {report}, Generation : {generation}, Person : {person}"
msgstr "Pozri správu : {report}, Generácia : {generation}, Osoba : {person}"

#, python-format
msgid "Report appearances for %s"
msgstr "Zobrazenia správy pre %s"

#, python-brace-format
msgid "Spouse of: Report: {report}, Generation: {generation}, Person: {person}"
msgstr ""
"Manžel/manželka osoby: Správa: {report}, Generácia: {generation}, Osoba: "
"{person}"

#, python-brace-format
msgid "Report: {report}, Generation: {generation}, Person: {person}"
msgstr "Správa: {report}, Generácia: {generation}, Osoba: {person}"

#, python-format
msgid "%(event_name)s of %(name)s "
msgstr "%(event_name)s osoby %(name)s "

#, python-format
msgid "%(event_name)s of %(name)s and %(mate)s "
msgstr "%(event_name)s osoby %(name)s a %(mate)s "

#, python-format
msgid "%(date)s, %(place)s"
msgstr "%(date)s, %(place)s"

#, python-format
msgid "%(date)s"
msgstr "%(date)s"

#, python-format
msgid "%(place)s"
msgstr "%(place)s"

#, python-format
msgid ". Ref: %(repno)s %(gen)s %(per)s "
msgstr ". Ref: %(repno)s %(gen)s %(per)s "

#, python-format
msgid "%(event_name)s:"
msgstr "%(event_name)s:"

#, python-format
msgid " %(event_text)s"
msgstr " %(event_text)s"

#, python-format
msgid "%(name_kind)s: %(name)s%(endnotes)s"
msgstr "%(name_kind)s: %(name)s%(endnotes)s"

#, python-format
msgid "%(type)s:"
msgstr "%(type)s:"

#, python-format
msgid " %(value)s%(endnotes)s"
msgstr " %(value)s%(endnotes)s"

msgid "List children"
msgstr "Uviesť deti"

msgid "Add descendant reference in child list"
msgstr "Pridať odkaz na potomka do zoznamu detí"

msgid "Include Index of Names"
msgstr "Zahrnúť index mien"

msgid "Whether to include the index of Names at the end of the report."
msgstr "Či zahrnúť index mien na konci správy."

msgid "Include index of Dates"
msgstr "Zahrnúť index dátumov"

msgid "Whether to include the index of Dates at the end of the report."
msgstr "Či zahrnúť index dátumov na konci správy."

msgid "Include index of Places"
msgstr "Zahrnúť index miest"

msgid "Whether to include the index of Places at the end of the report."
msgstr "Či zahrnúť index miest na konci správy."

msgid "Include the 'Report appearances' section with each person"
msgstr "Zahrnúť sekciu 'Zobrazenia v správe' pre každú osobu"

msgid ""
"The 'Report appearances' section shows other places in the report where the "
"person is mentioned."
msgstr ""
"Sekcia 'Zobrazenia v správe' zobrazuje ďalšie miesta v správe, kde je osoba "
"spomenutá."

msgid "The style used for the children list."
msgstr "Štýl použitý pre zoznam detí."

msgid "The basic style used for Note header."
msgstr "Základný štýl použitý pre záhlavie poznámky."

msgid "The style used for the first personal entry."
msgstr "Štýl použitý pre prvý osobný záznam."

msgid "The style used for the More About header and for headers of mates."
msgstr "Štýl použitý pre záhlavie Viac informácií a pre záhlavie partnerov."

msgid "The style used for additional detail data."
msgstr "Štýl použitý pre doplňujúce detailné údaje."

msgid "The style used for note detail data."
msgstr "Štýl použitý pre detailné údaje poznámky."

msgid "The style used for the date in the Index of Dates."
msgstr "Štýl použitý pre dátum v indexe dátumov."

msgid "The style used for the events in the Index of Dates."
msgstr "Štýl použitý pre udalosti v indexe dátumov."

msgid "The style used for the header in the Index of Names."
msgstr "Štýl použitý pre záhlavie v indexe mien."

msgid "The style used for the references in the Index of Names."
msgstr "Štýl použitý pre referencie v indexe mien."

msgid "The style used for the header in the Index of Places."
msgstr "Štýl použitý pre záhlavie v indexe miest."

msgid "The style used for the references in the Index of Places."
msgstr "Štýl použitý pre referencie v indexe miest."

