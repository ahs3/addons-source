msgid "Descendant Book"
msgstr "Nachkommenbuch"

msgid "Produces one or more descendant reports based on a supplied query."
msgstr ""
"Erstellt einen oder mehrere Nachkommenberichte basierend auf einer "
"mitgelieferten Abfrage."

msgid "Detailed Descendant Book"
msgstr "Ausführliches Nachkommenbuch"

msgid ""
"Produces one or more detailed descendant reports based on a supplied query."
msgstr ""
"Erstellt basierend auf einer angegebenen Abfrage einen oder mehrere "
"ausführliche Nachkommenberichte."

#, python-format
msgid "sp. see  %(reference)s : %(spouse)s"
msgstr "Part. siehe %(reference)s : %(spouse)s"

#, python-format
msgid "see report: %(report)s, ref: %(reference)s : %(person)s & %(spouse)s"
msgstr ""
"siehe Bericht: %(report)s, ref: %(reference)s : %(person)s & %(spouse)s"

msgid "Descendants Report"
msgstr "Nachkommenbericht"

#, python-format
msgid "Writing %s reports..."
msgstr "Schreibe %s Berichte..."

#, python-brace-format
msgid "{report_count}. Descendants of {name}"
msgstr "{report_count}. Nachkommen von {name}"

#, python-brace-format
msgid "{report_count:d}. {name}"
msgstr "{report_count:d}. {name}"

msgid "Select filter to restrict people that appear in the report"
msgstr ""
"Wählen Filter aus, um Personen einzuschränken, die im Bericht vorkommen"

msgid "Whether to show duplicate family trees in the report."
msgstr "Ob doppelte Stammbäume im Bericht angezeigt werden sollen."

msgid "The style used for the title of the page."
msgstr "Der für den Titel der Seite verwendete Stil."

msgid "The style used for the table of contents header."
msgstr ""
"Der Stil, der für die Überschrift des Inhaltsverzeichnisses verwendet wird."

msgid "The style used for the table of contents detail."
msgstr "Der Stil, der für die Angaben im Inhaltsverzeichnisses verwendet wird."

msgid "Detailed Descendants Report"
msgstr "Ausführlicher Nachkommenbericht"

#, python-format
msgid "Generating %s report references..."
msgstr "Erstelle %s Berichtsreferenzen..."

#, python-format
msgid "%(report_count)s. Descendant Report for %(person_name)s"
msgstr "%(report_count)s. Nachkommenbericht für %(person_name)s"

msgid "Index of Places"
msgstr "Ortsverzeichnis"

msgid "Index of Dates"
msgstr "Index der Daten"

msgid "Index of Names"
msgstr "Namensverzeichnis"

msgid "Report, Generation, Person, Name"
msgstr "Bericht, Generation, Person, Name"

#, python-brace-format
msgid "See Report : {report}, Generation : {generation}, Person : {person}"
msgstr "Siehe Bericht : {report}, Generation : {generation}, Person : {person}"

#, python-format
msgid "Report appearances for %s"
msgstr "Berichtserscheinungsbild für %s"

#, python-brace-format
msgid "Spouse of: Report: {report}, Generation: {generation}, Person: {person}"
msgstr ""
"Ehepartner von: Bericht: {report}, Generation: {generation}, Person:{person}"

#, python-brace-format
msgid "Report: {report}, Generation: {generation}, Person: {person}"
msgstr "Bericht: {report}, Generation: {generation}, Person: {person}"

