msgid ".html"
msgstr ".html"

#: D3Charts/DescendantIndentedTree.py:1928
msgid ".htm"
msgstr ".htm"

#: D3Charts/DescendantIndentedTree.py:1929
msgid ".shtml"
msgstr ".shtml"

