#, python-format
msgid "Failed to create directory structure : %s"
msgstr "Nepodarilo sa vytvoriť adresárovú štruktúru: %s"

#, python-format
msgid "Failed to copy web files : %s"
msgstr "Nepodarilo sa skopírovať webové súbory: %s"

msgid "Ancestral Collapsible Tree Options"
msgstr "Možnosti zbaliteľného stromu predkov"

msgid "Include Generations"
msgstr "Zahrnúť generácie"

msgid "Male Background Color"
msgstr "Pozadie pre mužov"

msgid "RGB-color for male box background."
msgstr "RGB-farba pre pozadie kolónky pre mužov."

msgid "Male Expandable Background Color"
msgstr "Farba rozbaľovacieho pozadia pre mužov"

msgid "RGB-color for male expandable box background."
msgstr "RGB-farba pozadia rozbaliteľnej kolónky pre mužov."

msgid "Female Background"
msgstr "Pozadie pre ženy"

msgid "RGB-color for female box background."
msgstr "RGB-farba pozadia kolónky pre ženy."

msgid "Female Expandable Background"
msgstr "Farba rozbaľovacieho pozadia pre ženy"

msgid "RGB-color for female expandable box background."
msgstr "RGB-farba pozadia rozbaliteľnej kolónky pre ženy."

msgid "The destination path for generated files."
msgstr "Cieľová cesta pre vygenerované súbory."

msgid "The destination file name for html content."
msgstr "Názov cieľového súboru pre obsah html."

msgid "Ancestral Collapsible Tree"
msgstr "Zbaliteľný strom predkov"

msgid ""
"Generates a web page with a graphical representation of ancestors (SVG) "
"represented as a Collapsible Tree Layout from the D3.js JavaScript library."
msgstr ""
"Vygeneruje webovú stránku s grafickým znázornením predkov (SVG) "
"reprezentovaných ako Collapsible Tree Layout z knižnice JavaScript D3.js."

msgid "Descendant Indented Tree"
msgstr "Členitý strom potomkov"

msgid ""
"Generates a web page with a graphical representation of descendants (SVG) "
"representedas a Collapsible Indented Tree Layout from the D3.js JavaScript "
"library."
msgstr ""
"Vygeneruje webovú stránku s grafickým znázornením potomkov (SVG) "
"reprezentovaných ako Collapsible Indented Tree Layout z knižnice JavaScript "
"D3.js."

msgid "Ancestral Fan Chart Options"
msgstr "Možnosti vejárového grafu predkov"

msgid "Paternal Background Color"
msgstr "Farba pozadia otcovskej línie"

msgid "RGB-color for paternal box background."
msgstr "RGB-farba pre pozadie kolónky otcovskej línie."

msgid "Maternal Background"
msgstr "Pozadie materskej línie"

msgid "RGB-color for maternal box background."
msgstr "RGB -farba pre pozadie kolónky materskej línie."

msgid "Ancestral Fan Chart"
msgstr "Vejárový graf predkov"

msgid ""
"Generates a web page with a graphical representation of ancestors (SVG) "
"represented as a Fan Chart from the D3.js JavaScript library."
msgstr ""
"Generuje webovú stránku s grafickým znázornením predkov (SVG) "
"reprezentovaných ako vejárový graf z knižnice JavaScript D3.js."

#, python-format
msgid "See %(reference)s : %(spouse)s"
msgstr "Pozri %(reference)s : %(spouse)s"

msgid "Generating report..."
msgstr "Generovanie správy..."

msgid "Invalid Destination Directory"
msgstr "Neplatný cieľový adresár"

#, python-format
msgid ""
"Destinaton diretory %s does not exist\n"
"Do you want to attempt to create it?"
msgstr ""
"Cieľový adresár %s neexistuje.\n"
"Chcete sa pokúsiť ho vytvoriť?"

#, python-format
msgid ""
"Destination file %s already exists.\n"
"Do you want to overwrite it?"
msgstr ""
"Cieľový súbor %s už existuje.\n"
"Chcete ho prepísať?"

msgid "Default View"
msgstr "Predvolené zobrazenie"

msgid "Expand All"
msgstr "Rozbaliť všetko"

msgid "Include contraction level"
msgstr "Zahrnúť úroveň zbalenia"

msgid "The number of descendant levels to contract on initial display."
msgstr "Počet úrovní potomkov, ktoré sa majú zbaliť pri úvodnom zobrazení."

msgid "The font size in pixels for each node."
msgstr "Veľkosť písma v pixeloch pre každý uzol."

msgid "Whether to show duplicate family trees in the report."
msgstr "Či sa majú v správe zobraziť duplicitné rodinné stromy."

msgid "Expanded Row Background Color"
msgstr "Farba pozadia pre rozbalený riadok"

msgid "RGB-color for expanded row background."
msgstr "RGB-farba pre pozadie rozbaleného riadku."

msgid "Expandable Background Color"
msgstr "Farba rozbaliteľného pozadia"

msgid "RGB-color for expandable row background."
msgstr "RGB-farba pre pozadie rozbaliteľného riadku."

msgid "Non-Expandable Background Color"
msgstr "Farba pozadia pre nerozbaliteľné"

msgid "RGB-color for non-expandable row background."
msgstr "RGB-farba pre pozadie nerozbaliteľného riadku."

msgid "Include records marked private"
msgstr "Zahrnúť záznamy označené ako súkromné"

msgid "Whether to include private objects."
msgstr "Či zahrnúť súkromné objekty."

msgid "Include Last Name Only"
msgstr "Zahrnúť iba priezvisko"

msgid "Include Full Name Only"
msgstr "Zahrnúť iba celé meno"

msgid "How to handle living people."
msgstr "Ako narábať so žijúcimi osobami."

msgid "Whether or not to include people who may have recently died."
msgstr "Či zahrnúť osoby, ktoré mohli nedávno zomrieť."

msgid "Navigation"
msgstr "Navigácia"

msgid "Contraction/Expansion mechanism"
msgstr "Mechanizmus zbalenia/rozbalenia"

msgid "Entire node."
msgstr "Celý uzol."

msgid "Arrow images at start of node."
msgstr "Obrázky so šípkami na začiatku uzla."

msgid "Plus/Minus images at start of node."
msgstr "Obrázky plus/mínus na začiatku uzla."

msgid "Where to click to expand/contract nodes."
msgstr "Kde kliknúť pre rozbalenie/zbalenie uzlov."

msgid "Auto-generate HREF links for nodes"
msgstr "Automaticky generovať HREF odkazy pre uzly"

msgid "Whether to auto-generate HTML links for each node on the report."
msgstr "Či automaticky generovať HTML odkazy pre každý uzol v správe."

msgid "URL prefix path."
msgstr "Predpona URL cesty."

msgid "URL prefix to apply to each auto-generated HREF link."
msgstr ""
"Predpona URL, ktorá sa má použiť pre každý automaticky generovaný HREF odkaz."

msgid "URL file extension"
msgstr "Prípona URL súboru"

msgid "No file extension."
msgstr "Žiadna prípona súboru."

msgid ".html"
msgstr ".html"

msgid ".htm"
msgstr ".htm"

msgid ".shtml"
msgstr ".shtml"

msgid ".php"
msgstr ".php"

msgid ".php3"
msgstr ".php3"

msgid ".cgi"
msgstr ".cgi"

msgid "URL file name delimeter"
msgstr "Oddeľovač názvu URL súboru"

msgid "Remove all whitespace."
msgstr "Odstrániť všetky prázdne znaky."

msgid "Replace whitespace with dash(-)."
msgstr "Nahradiť prázdne znaky pomlčkou (-)."

msgid "Replace whitespace with underscore(_)"
msgstr "Nahradiť prázdne znaky podčiarkovníkom (_)"

msgid "What to use as file name delimiter replacing whitespace."
msgstr "Čo použiť ako oddeľovač názvu súboru nahrádzajúci prázdne znaky."

msgid "Generations to generate HREF links"
msgstr "Generácie na generovanie HREF odkazov"

msgid "The number of generations to generate HREF links."
msgstr "Počet generácií na generovanie HREF odkazov."

msgid "Exclude persons who died before (years)"
msgstr "Vylúčiť osoby, ktoré zomreli pred (years)"

msgid ""
"Whether to generate links for people who died in infancy, -1 excludes nobody."
msgstr ""
"Či generovať odkazy pre ľudí, ktorí zomreli v detstve, -1 nikoho nevylučuje."

msgid "Exclude center person"
msgstr "Vylúčiť ústrednú osobu"

msgid "Whether or not to generate links for center person."
msgstr "Či generovať odkazy pre ústrednú osobu."

msgid "Exclude Spouses"
msgstr "Vylúčiť manželov/manželky"

msgid "Whether or not to generate links for spouses."
msgstr "Či generovať odkazy pre manželov/manželky."

msgid "Biography Content"
msgstr "Obsah biografie"

msgid "Show biography tooltips"
msgstr "Zobraziť nápovedu k biografii"

msgid "Whether to show biography tooltips when hovering over a node."
msgstr "Či zobraziť nápovedu k biografii pri prechode kurzorom myši nad uzlom."

msgid "Text Color"
msgstr "Farba textu"

msgid "RGB-color for biography text."
msgstr "RGB-farba pre text biografie."

msgid "RGB-color for biography tooltip."
msgstr "RGB-farba pre nápovedu k biografii."

msgid "Header Font size"
msgstr "Veľkosť písma záhlavia"

msgid "The font size in pixels for biography header text."
msgstr "Veľkosť písma v pixeloch pre text záhlavia biografie."

msgid "Body Font size"
msgstr "Veľkosť písma hlavného textu"

msgid "The font size in pixels for biography body text."
msgstr "Veľkosť písma v pixeloch pre hlavný text biografie."
