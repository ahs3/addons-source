#: DynamicWeb/dynamicweb.py:2538 DynamicWeb/dynamicweb.py:3052
#: SetPrivacyTool/SetPrivacyTool.py:195
msgid "Persons"
msgstr "Personen"

#: SetPrivacyTool/SetPrivacyTool.gpr.py:23 SetPrivacyTool/SetPrivacyTool.py:48
#: SetPrivacyTool/SetPrivacyTool.py:93 SetPrivacyTool/SetPrivacyTool.py:130
#: SetPrivacyTool/SetPrivacyTool.py:157 SetPrivacyTool/SetPrivacyTool.gpr.py:24
#: SetPrivacyTool/SetPrivacyTool.py:51 SetPrivacyTool/SetPrivacyTool.py:96
#: SetPrivacyTool/SetPrivacyTool.py:146 SetPrivacyTool/SetPrivacyTool.py:186
#: SetPrivacyTool/SetPrivacyTool.py:226 SetPrivacyTool/SetPrivacyTool.py:255
msgid "Set Privacy Tool"
msgstr "Datenschutz einstellen Werkzeug"

#: SetPrivacyTool/SetPrivacyTool.gpr.py:24
#: SetPrivacyTool/SetPrivacyTool.gpr.py:25
msgid "Set all objects of the last <number> of years private."
msgstr "All Objekte der letzten <number> Jahre als vertraulich markieren."

#: SetPrivacyTool/SetPrivacyTool.py:87 SetPrivacyTool/SetPrivacyTool.py:90
#, python-format
msgid "Set private: %d %s\n"
msgstr "Vertraulich einstellen: %d %s\n"

#: SetPrivacyTool/SetPrivacyTool.py:89 SetPrivacyTool/SetPrivacyTool.py:92
#, python-format
msgid "Not private: %d %s\n"
msgstr "Nicht vertraulich: %d %s\n"

#: SetPrivacyTool/SetPrivacyTool.py:96 SetPrivacyTool/SetPrivacyTool.py:107
msgid "Set persons private.."
msgstr "Personen als vertraulich markieren…"

#: SetPrivacyTool/SetPrivacyTool.py:133 SetPrivacyTool/SetPrivacyTool.py:157
msgid "Set events private.."
msgstr "Ereignisse als vertraulich markieren…"

#: SetPrivacyTool/SetPrivacyTool.py:160 SetPrivacyTool/SetPrivacyTool.py:197
msgid "Set media private.."
msgstr "Medien als vertraulich markieren…"

#: SetPrivacyTool/SetPrivacyTool.py:204 SetPrivacyTool/SetPrivacyTool.py:306
msgid "Always private if no date."
msgstr "Immer vertraulich, wenn keine Datumsangabe."

#: SetPrivacyTool/SetPrivacyTool.py:205 SetPrivacyTool/SetPrivacyTool.py:307
msgid "If checked, all objects without a date will also be set private."
msgstr ""
"Wenn aktiviert, werden alle Objekte ohne Datum ebenfalls als vertraulich "
"markiert."

#: SetPrivacyTool/SetPrivacyTool.py:210 SetPrivacyTool/SetPrivacyTool.py:301
msgid ""
"The time range in years from today you want to set objects private.\n"
"'0 years' = remove privacy from all objects."
msgstr ""
"Zeitspanne in Jahren ab heute, in der Objekte als vertraulich markiert "
"werden.\n"
"\"0 Jahre\" = entferne die Vertraulichkeit von allen Objekten."
