#: ImportMerge/importmerge.gpr.py:30 ImportMerge/importmerge.py:311
#: ImportMerge/importmerge.py:690 ImportMerge/importmerge.gpr.py:31
#: ImportMerge/importmerge.py:328 ImportMerge/importmerge.py:707
msgid "Import and Merge tool"
msgstr "Alat za uvoz i sjedinjavanje"

#: ImportMerge/importmerge.gpr.py:31 ImportMerge/importmerge.gpr.py:33
msgid ""
"Compares a Gramps XML database with the current one, and allows merging of "
"the differences."
msgstr ""
"Uspoređuje Gramps XML bazu podataka s trenutačnom i dozvoljava njihovo "
"sjedinjenje."

#: ImportMerge/importmerge.py:80
msgid "Import and merge a Gramps XML"
msgstr "Uvezi i sjedini Gramps XML"

#: ImportMerge/importmerge.py:89
msgid "Missing"
msgstr "Nedostaje"

#: ImportMerge/importmerge.py:93
msgid "Different"
msgstr "Različito"

#: ImportMerge/importmerge.py:108
msgid "Delete original"
msgstr "Obriši original"

#: ImportMerge/importmerge.py:109 ImportMerge/importmerge.glade:332
#: ImportMerge/importmerge.glade:334
msgid "Ignore"
msgstr "Zanemari"

#: ImportMerge/importmerge.py:110
msgid "Add Import"
msgstr "Dodaj uvoz"

#: ImportMerge/importmerge.py:111
msgid "Merge into original"
msgstr "Sjedini u original"

#: ImportMerge/importmerge.py:112
msgid "Merge into import"
msgstr "Sjedini u uvoz"

#: ImportMerge/importmerge.py:113
msgid "Replace with import"
msgstr "Zamijeni s uvozom"

#: ImportMerge/importmerge.py:114
msgid "Keep original"
msgstr "Zadrži original"

#: ImportMerge/importmerge.py:115 ImportMerge/importmerge.glade:291
#: ImportMerge/importmerge.glade:293
msgid ""
"Use buttons below to set the 'Action' for each difference.  No changes will "
"be made to your tree until you press 'Done' and confirm."
msgstr ""
"Koristi donje gumbove i postavi polje „Radnja” za svaku razliku pojedinačno. "
"Promjene u rodoslovu stupit će na snagu tek nakon pritiskanja gumba „Gotovo”."

#: ImportMerge/importmerge.py:118
msgid ""
"This item will be deleted from your tree.  Any referenced items were also "
"marked for deletion."
msgstr ""
"Stavke će biti izbrisane iz tvog rodoslova. Sve referencirajuće stavke su "
"također označene za brisanje."

#: ImportMerge/importmerge.py:120
msgid ""
"This item will not be changed in your tree.  Any referenced items were also "
"marked for Ignore."
msgstr ""
"Ova stavka se neće mijenjati u tvom rodoslovu. Sve referencirajuće stavke su "
"također označene sa „Zanemari”."

#: ImportMerge/importmerge.py:122
msgid ""
"This item will be added to your tree.  Any referenced items were also marked "
"for adding."
msgstr ""
"Ova stavka će biti dodana tvom rodoslovu. Sve referencirajuće stavke su "
"također označene za dodavanje."

#: ImportMerge/importmerge.py:124
msgid ""
"This item will be merged, saving data from your tree.  Any referenced items "
"were also marked for merging or adding."
msgstr ""
"Ova će se stavka sjediniti, spremajući podatke iz tvog rodoslova. Sve "
"referencirajuće stavke također su označene za sjedinjavanje ili dodavanje."

#: ImportMerge/importmerge.py:126
msgid ""
"This item will be merged, using data from the import.  Any referenced items "
"were also marked for merging or adding."
msgstr ""
"Ova će se stavka sjediniti, koristeći podatke iz uvoza. Sve referencirajuće "
"stavke također su označene za sjedinjavanje ili dodavanje."

#: ImportMerge/importmerge.py:128
msgid ""
"The import data will entirely replace the data in your tree.  Any referenced "
"items were also marked for replacement or removal."
msgstr ""
"Uvozni podaci će u potpunosti zamijeniti podatke u tvom rodoslovu. Sve "
"referencirajuće stavke su također označene za zamjenu ili uklanjanje."

#: ImportMerge/importmerge.py:130
msgid ""
"This item will not be changed in your tree.  Any referenced items were also "
"marked to keep."
msgstr ""
"Ova stavka se neće mijenjati u tvom rodoslovu. Sve referencirajuće stavke su "
"također označene za zadržavanje."

#: ImportMerge/importmerge.py:321 ImportMerge/importmerge.py:338
msgid "Import Failure"
msgstr "Greška prilikom uvoza"

#: ImportMerge/importmerge.py:389 ImportMerge/importmerge.py:406
msgid "Your Tree and import are the same."
msgstr "Tvoj rodoslov i uvoz su jednaki."

#: ImportMerge/importmerge.py:434 ImportMerge/importmerge.py:443
#: ImportMerge/importmerge.py:451 ImportMerge/importmerge.py:459
#: ImportMerge/importmerge.py:460 ImportMerge/importmerge.py:468
#: ImportMerge/importmerge.py:476
msgid "your tree "
msgstr "tvoj rodoslov "

#: ImportMerge/importmerge.py:441 ImportMerge/importmerge.py:449
#: ImportMerge/importmerge.py:457 ImportMerge/importmerge.py:458
#: ImportMerge/importmerge.py:466 ImportMerge/importmerge.py:474
msgid "imported "
msgstr "uvezeno "

#: ImportMerge/importmerge.py:464 ImportMerge/importmerge.py:943
#: ImportMerge/importmerge.py:964 ImportMerge/importmerge.py:481
#: ImportMerge/importmerge.py:960 ImportMerge/importmerge.py:981
msgid "Original"
msgstr "Original"

#: ImportMerge/importmerge.py:465 ImportMerge/importmerge.py:944
#: ImportMerge/importmerge.py:965 ImportMerge/importmerge.py:482
#: ImportMerge/importmerge.py:961 ImportMerge/importmerge.py:982
msgid "Imported"
msgstr "Uvezeno"

#: ImportMerge/importmerge.py:467 ImportMerge/importmerge.py:946
#: ImportMerge/importmerge.py:967 ImportMerge/importmerge.py:484
#: ImportMerge/importmerge.py:963 ImportMerge/importmerge.py:984
msgid "Result  "
msgstr "Rezultat "

#: ImportMerge/importmerge.py:693 ImportMerge/importmerge.py:710
msgid "Import and Merge"
msgstr "Uvezi i sjedini"

#: ImportMerge/importmerge.glade:8
msgid ""
"A '*' indicates that the action was automarked, a '?' indicates a conflict "
"between automarks."
msgstr ""
"Zvijezdica (*) ukazuje na automatski označenu radnju, a upitnik (?) ukazuje "
"na konflikt između automatskih oznaka."

#: ImportMerge/importmerge.glade:38 ImportMerge/importmerge.glade:39
msgid "Import file"
msgstr "Uvezi datoteku"

#: ImportMerge/importmerge.glade:122 ImportMerge/importmerge.glade:124
msgid "Select the difference you wish to examine."
msgstr "Odaberi raziku koju želiš provjeriti."

#: ImportMerge/importmerge.glade:178 ImportMerge/importmerge.glade:180
msgid "Object Name/Description"
msgstr "Ime objekta/Opis"

#: ImportMerge/importmerge.glade:225 ImportMerge/importmerge.glade:227
msgid ""
"Data item details, showing the current treeitem, imported tree item, and "
"result (when chosen)."
msgstr ""
"Detalji stavke, prikazujući trenutačnu stavku, uvezenu stavku i rezultat "
"(ukoliko je odabrano)."

#: ImportMerge/importmerge.glade:246 ImportMerge/importmerge.glade:248
msgid "Object and item"
msgstr "Objekt i stavka"

#: ImportMerge/importmerge.glade:336 ImportMerge/importmerge.glade:338
msgid "This button will ignore the difference, no changes to your tree."
msgstr "Ovaj gumb će zanemariti razlike, bez promjena u tvom rodoslovu."

#: ImportMerge/importmerge.glade:347 ImportMerge/importmerge.glade:349
msgid "Unmark"
msgstr "Odznači"

#: ImportMerge/importmerge.glade:351 ImportMerge/importmerge.glade:353
msgid "This button will add the imported data as a new object to your tree."
msgstr "Ovaj gumb će dodati uvezene podatke u tvoj rodoslov kao nove objekte."

#: ImportMerge/importmerge.glade:366 ImportMerge/importmerge.glade:368
msgid ""
"This button will bring up a dialog allowing you to finalize your changes, or "
"abandon them."
msgstr ""
"Ovaj gumb će otvoriti dijaloški okvir, u kojem tvoje promjene možeš završtit "
"ili ih se odreći."

#: ImportMerge/importmerge.glade:390 ImportMerge/importmerge.glade:392
msgid "Merge Original"
msgstr "Sjedini original"

#: ImportMerge/importmerge.glade:394 ImportMerge/importmerge.glade:396
msgid ""
"This button will merge the imported data into your tree with your tree data "
"as a base."
msgstr ""
"Ovaj gumb će sjediniti uvezene podatke s tvojim rodoslovom, a rodoslovni "
"podaci se uzimaju kao osnova."

#: ImportMerge/importmerge.glade:409 ImportMerge/importmerge.glade:411
msgid "This button will replace your tree data with the imported data."
msgstr "Ovaj gumb će zamijeniti podatke u tvom rodoslovu s uvezenim podacima."

#: ImportMerge/importmerge.glade:420 ImportMerge/importmerge.glade:422
msgid "Merge Import"
msgstr "Sjedini uvoz"

#: ImportMerge/importmerge.glade:424 ImportMerge/importmerge.glade:426
msgid ""
"This button will merge the imported data into your tree with the imported "
"data as a base."
msgstr ""
"Ovaj gumb će sjediniti uvezene podatke s tvojim rodoslovom, a uvozni podaci "
"se uzimaju kao osnova."

#: ImportMerge/importmerge.glade:435 ImportMerge/importmerge.glade:437
msgid "Edit Import"
msgstr "Uredi uvoz"

#: ImportMerge/importmerge.glade:439 ImportMerge/importmerge.glade:441
msgid ""
"This button will edit the imported data.  The data will still need to be "
"merged, added or replaced."
msgstr ""
"Ovaj gumb će urediti uvezene podatke. Podaci će se svejedno morati "
"sjediniti, dodati ili zamijeniti."

#: ImportMerge/importmerge.glade:462 ImportMerge/importmerge.glade:464
msgid "Show more details"
msgstr "Prikaži više detalja"

#: ImportMerge/importmerge.glade:479 ImportMerge/importmerge.glade:481
msgid "Automark Families"
msgstr "Automatski označi obitelji"

#: ImportMerge/importmerge.glade:496 ImportMerge/importmerge.glade:498
msgid "Automark Parent Families"
msgstr "Automatski označi obitelji roditelja"
