msgid "Data Entry Gramplet"
msgstr "データ入力Gramplet"

msgid "Gramplet for quick data entry"
msgstr "素早いデータ入力のためのGramplet"

msgid "Active person"
msgstr "アクティブな人"

msgid "Family:"
msgstr "家族 :"

msgid "Sources?"
msgstr "ソース?"

msgid "Surname, Given"
msgstr "姓、名"

msgid "Save"
msgstr "保存"

msgid "Abandon"
msgstr "放棄"

msgid "New person"
msgstr "新しい人"

msgid "Add relation"
msgstr "関係を追加"

msgid "No relation to active person"
msgstr "アクティブな人とは関係ない"

msgid "Add as a Parent"
msgstr "親として追加"

msgid "Add as a Mother"
msgstr "母親として追加"

msgid "Add as a Father"
msgstr "父親として追加"

msgid "Add as a Spouse"
msgstr "配偶者として追加"

msgid "Add as a Wife"
msgstr "妻として追加"

msgid "Add as a Husband"
msgstr "夫として追加"

msgid "Add as a Sibling"
msgstr "兄弟として追加"

msgid "Add as a Child"
msgstr "子供として追加"

msgid "Copy Active Data"
msgstr "アクティブデータのコピー"

#, python-format
msgid "Gramplet Data Edit: %s"
msgstr "Gramplet データ編集 : %s"

msgid "Please provide a name."
msgstr "名前を入力してください。"

msgid "Can't add new person."
msgstr "新しい人を追加できません。"

msgid "Please set an active person."
msgstr "アクティブな人を設定してください。"

msgid "Can't add new person as a parent."
msgstr "新しい人を親として追加できません。"

msgid "Please set the new person's gender."
msgstr "新しい人の性別を設定してください。"

msgid "Can't add new person as a mother."
msgstr "新しい人を母親として追加することはできません。"

msgid "Can't add new person as a father."
msgstr "新しい人を父親として追加することはできません。"

msgid "Can't add new person as a spouse."
msgstr "新しい人を配偶者として追加することはできません。"

msgid "Can't add new person as a wife."
msgstr "新しい人を妻として追加できません。"

msgid "Can't add new person as a husband."
msgstr "新しい人を夫として追加することはできません。"

msgid "Can't add new person as a sibling."
msgstr "新しい人を兄弟として追加することはできません。"

msgid "Can't add new person as a child."
msgstr "新しい人を子供として追加することはできません。"

msgid "Please set Active person."
msgstr "アクティブユーザーを設定してください。"

msgid "Can't add new person as a spouse to no one."
msgstr "誰にも新しい人を配偶者として追加することはできません。"

msgid "Please set gender on Active or new person."
msgstr "性別をアクティブまたは新規ユーザーに設定してください。"

msgid "Same genders on Active and new person."
msgstr "アクティブユーザーと新規ユーザーの性別は同じです。"

msgid "Please set gender on Active person."
msgstr "性別をアクティブな人に設定してください。"
