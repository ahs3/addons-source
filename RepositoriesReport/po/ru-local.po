msgstr "Выбор с использованием фильтра"

msgid "Include repository's urls"
msgstr "Включать сслыки на хранилище"

msgid "Whether to include urls on repository."
msgstr "Включать ли ссылки на хранилище."

msgid "Include repository's address"
msgstr "Включать адрес хранилища"

msgid "Whether to include addresses on repository."
msgstr "Включать ли адрес хранилища."

msgid "Include source's author"
msgstr "Включать автора источника"

msgid "Whether to include author."
msgstr "Включать ли автора."

msgid "Include source's abbreviation"
msgstr "Включать аббревиатуру источника"

msgid "Whether to include abbreviation."
msgstr "Включать ли аббревиатуру."

msgid "Include source's publication information"
msgstr "Включать информацию о публикации источника"

