#: DynamicWeb/dynamicweb.py:2607 DynamicWeb/dynamicweb.py:3170
#: LifeLineChartView/lifelinechartview.py:508
msgid "Zoom in"
msgstr "Hineinzoomen"

#: DynamicWeb/dynamicweb.py:2608 DynamicWeb/dynamicweb.py:3171
#: LifeLineChartView/lifelinechartview.py:518
msgid "Zoom out"
msgstr "Herauszoomen"

#: DynamicWeb/dynamicweb.py:4177 DynamicWeb/dynamicweb.py:5098
#: LifeLineChartView/_dummy_translation_string_po.py:101
msgid "Maximum number of generations"
msgstr "Maximale Anzahl der Generationen"

#: LifeLineChartView//_dummy_translation_string_po.py:10
#: LifeLineChartView/_dummy_translation_string_po.py:10
msgid "Warp the chart shape"
msgstr "Diagramm verzerren"

#: LifeLineChartView//_dummy_translation_string_po.py:11
#: LifeLineChartView/_dummy_translation_string_po.py:11
msgid "The overall shape of the chart can be warped."
msgstr "Die gesamte Form des Diagramms kann verzerrt werden."

#: LifeLineChartView//_dummy_translation_string_po.py:12
#: LifeLineChartView/_dummy_translation_string_po.py:12
msgid "Normal grid shape"
msgstr "Normale Gitterform"

#: LifeLineChartView//_dummy_translation_string_po.py:13
#: LifeLineChartView/_dummy_translation_string_po.py:13
msgid "Sine shape"
msgstr "Sinusform"

#: LifeLineChartView//_dummy_translation_string_po.py:14
#: LifeLineChartView/_dummy_translation_string_po.py:14
msgid "Triangular shape"
msgstr "Dreiecksform"

#: LifeLineChartView//_dummy_translation_string_po.py:15
#: LifeLineChartView/_dummy_translation_string_po.py:15
msgid "Show photos"
msgstr "Fotos anzeigen"

#: LifeLineChartView//_dummy_translation_string_po.py:16
#: LifeLineChartView/_dummy_translation_string_po.py:16
msgid "Photos of the individual are shown."
msgstr "Fotos der Person werden gezeigt."

#: LifeLineChartView//_dummy_translation_string_po.py:17
#: LifeLineChartView/_dummy_translation_string_po.py:17
msgid "Photo size"
msgstr "Größe der Fotos"

#: LifeLineChartView//_dummy_translation_string_po.py:18
#: LifeLineChartView/_dummy_translation_string_po.py:18
msgid ""
"Photos which are shown are fitted into a square of this extent. The size is "
"given relative to the line thickness."
msgstr ""
"Die Bilder werden in eine Quadrat dieser Kantenlänge eingepasst. Die Länge "
"wird relativ zur Linienbreite angegeben."

#: LifeLineChartView//_dummy_translation_string_po.py:19
#: LifeLineChartView/_dummy_translation_string_po.py:19
msgid "Total height"
msgstr "Gesamthöhe"

#: LifeLineChartView//_dummy_translation_string_po.py:20
#: LifeLineChartView/_dummy_translation_string_po.py:20
msgid "Total height of the whole chart."
msgstr "Höhe des gesamten Diagramms."

#: LifeLineChartView//_dummy_translation_string_po.py:21
#: LifeLineChartView/_dummy_translation_string_po.py:21
msgid "Relative line thickness"
msgstr "Relative Linienbreite"

#: LifeLineChartView//_dummy_translation_string_po.py:22
#: LifeLineChartView/_dummy_translation_string_po.py:22
msgid ""
"The line thickness of an individual is given relatively to the horizontal "
"step size."
msgstr ""
"Die Breite eine Lebenslinie wird relativ zur horizontalen Schrittweite "
"angegeben."

#: LifeLineChartView//_dummy_translation_string_po.py:23
#: LifeLineChartView/_dummy_translation_string_po.py:23
msgid "Horizontal step size"
msgstr "Horizontale Schrittweite"

#: LifeLineChartView//_dummy_translation_string_po.py:24
#: LifeLineChartView/_dummy_translation_string_po.py:24
msgid ""
"This is the distance from one line to another. This value is also used for "
"scaling of other items."
msgstr ""
"Dies ist der Abstand zwischen zwei Lebenslinien. Dieser Wert wird häufig zur "
"Skalierung anderer Objekte verwendet."

#: LifeLineChartView//_dummy_translation_string_po.py:25
#: LifeLineChartView/_dummy_translation_string_po.py:25
msgid "Show birth label"
msgstr "Geburts-Textfeld anzeigen"

#: LifeLineChartView//_dummy_translation_string_po.py:26
#: LifeLineChartView/_dummy_translation_string_po.py:26
msgid "Activate the birth label."
msgstr "Aktiviert einen kurzen Infotextes zur Geburt."

#: LifeLineChartView//_dummy_translation_string_po.py:27
#: LifeLineChartView/_dummy_translation_string_po.py:27
msgid "Birth label along path"
msgstr "Geburts-Textfeld auf der Linie"

#: LifeLineChartView//_dummy_translation_string_po.py:28
#: LifeLineChartView/_dummy_translation_string_po.py:28
msgid "The birth label is aligned to the individual line."
msgstr "Das Geburts-Textfeld wird entlang der Linie dargestellt."

